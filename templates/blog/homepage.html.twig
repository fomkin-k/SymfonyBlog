{% extends 'base.html.twig' %}

{% block body_id 'homepage' %}

{% block body %}
  <div class="container"> 
    <article class="post">  
    {% for user in users %}
      <h3><a href="{{ path('app_user_posts', {login: user.login}) }}">{{ user.full_name }}</a></h2>
      
      <p>{{ user.email }}
      </p>
    {% endfor %}
    </article>
  </div>
{% endblock %}
